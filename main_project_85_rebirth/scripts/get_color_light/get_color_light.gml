/// @description get_color_light(color name)
/// @param color name
if argument0 = "original"
{
return global.original_light;
}

if argument0 = "sepia"
{
return global.sepia_light;
}

if argument0 = "greeny"
{
return global.greeny_light;
}

if argument0 = "oldy"
{
return global.oldy_light;
}

if argument0 = "clive"
{
return global.clive_light;
}

if argument0 = "blue"
{
return global.blue_light;
}

if argument0 = "rainbow"
{
return global.rainbow_light;
}

if argument0 = "low contrast"
{
return global.low_contrast_light;
}

